<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">


	<listener>
		<listener-class>com.jyz.utils.ListenerLoadConfig</listener-class>
	</listener>

	<servlet>
		<servlet-name>Test</servlet-name>
		<servlet-class>com.jyz.servlets.Test</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>Test</servlet-name>
		<url-pattern>/Test</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RegistUser</servlet-name>
		<servlet-class>com.jyz.servlets.RegistUser</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>RegistUser</servlet-name>
		<url-pattern>/RegistUser</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LoginUser</servlet-name>
		<servlet-class>com.jyz.servlets.LoginUser</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>LoginUser</servlet-name>
		<url-pattern>/LoginUser</url-pattern>
	</servlet-mapping>

</web-app>


	